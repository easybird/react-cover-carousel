{
  "name": "react-cover-carousel",
  "version": "0.0.1",
  "description": "React Stylish Cover Carousel Library",
  "main": "build/react-cover-carousel.js",
  "module": "build/react-cover-carousel.module.js",
  "scripts": {
    "build": "rollup -c rollup.config.babel.js --environment BUILD:production",
    "watch": "rollup -c rollup.config.babel.js -w --environment BUILD:development",
    "preinstall": "cd \"./examples\" && npm install",
    "examples:build": "cd \"./examples\" && \"./node_modules/.bin/webpack\" --mode production",
    "examples:watch": "cd \"./examples\" && \"./node_modules/.bin/webpack-dev-server\" --mode development --watch",
    "examples:deploy": "npm run preinstall && npm run build && npm run examples:build",
    "start": "(npm run watch & npm run examples:watch)",
    "precommit": "npm run build && npm run examples:build",
    "test": "jest --no-cache"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "carousel",
    "cover"
  ],
  "author": "easybird",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.28.11",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "postcss-scss": "^1.0.5",
    "react-test-renderer": "^16.4.0",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1"
  },
  "dependencies": {
    "radium": "^0.24.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  }
}
